{  
  "name": "京豆",  
  "manifest_version": 2,  
  "version": "1.5",  
  "description": "自动抢京豆，方便你我他",  
  "browser_action": {  
    "default_icon": "dou.png",
	"default_title": "京豆1.5"
  },
  "permissions": [  
        "tabs", "http://*/*","https://*/*"  
  ],  
  "content_scripts": [    
   {    
      "matches": ["http://*/*","https://*/*"],    
      "js": ["jquery-2.1.4.min.js","app.js"]    
   }    
  ]    
}  